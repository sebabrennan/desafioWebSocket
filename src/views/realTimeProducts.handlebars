<H3>REAL TIME PRODUCTS</H3>
<p id="list"></p>

<form id="addProductForm">
    <p>Ingresar datos del producto:</p>
    <div>
        <label for="title">Title:</label>
        <input type="text" id="title" placeholder="Title">
    </div>
    <div>
        <label for="category">Category:</label>
        <input type="text" id="category" placeholder="Category">
    </div>
    <div>
        <label for="description">Description:</label>
        <input type="text" id="description" placeholder="Description">
    </div>
    <div>
        <label for="price">Price:</label>
        <input type="number" id="price" placeholder="Price">
    </div>
    <div>
        <label for="code">Code:</label>
        <input type="text" id="code" placeholder="Code">
    </div>
    <div>
        <label for="stock">Stock:</label>
        <input type="number" id="stock" placeholder="Stock">
    </div>
    <div>
        <button type="submit" id="addProduct">Agregar</button>
    </div>
</form>

<form id="deleteProductForm">
    <p>Eliminar un producto</p>
    <div>
        <label for="id">ID:</label>
        <input type="text" id="id" placeholder="Ingrese ID del producto a eliminar">
    </div>
    <div>
        <button id="deleteProduct" type="submit">Eliminar</button>
    </div>
</form>



<script src="/socket.io/socket.io.js" charset="utf-8"></script>
<script src="/js/index.js" charset="utf-8"></script>